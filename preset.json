{
  "useConfigFiles": true,
  "useTaobaoRegistry": false,
  "packageManager": "yarn",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": ["save"]
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "vue-cli-plugin-lego": {
      "prompts": true
    }
  },
  "cssPreprocessor": "dart-sass"
}
